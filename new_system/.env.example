# Excel Q&A Dataset Pipeline Configuration
# Copy this file to .env and fill in your API keys

# Stack Overflow API Configuration
STACKOVERFLOW_API_KEY=your_stackoverflow_api_key_here

# OpenRouter API Configuration (for AI image enhancement)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Custom paths (defaults will be used if not specified)
# OUTPUT_DIR=./data/output
# TEMP_DIR=./data/temp
# LOG_DIR=./logs
# DATABASE_PATH=./data/cache.db

# Optional: Processing limits
# MAX_DAILY_REQUESTS=10000
# COLLECTION_BATCH_SIZE=100
# QUALITY_THRESHOLD=70

# Optional: Development settings
# DEBUG=false
# NODE_ENV=production